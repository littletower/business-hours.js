{
  "name": "business-hours.js",
  "version": "2.0.1",
  "description": "Business hours javascript lib",
  "keywords": [
    "business hours",
    "hours",
    "opening hours",
    "opening",
    "operating hours",
    "operating",
    "holidays"
  ],
  "main": "dist/index.js",
  "scripts": {
    "report-coverage":
      "nyc report --reporter=lcov | codecov -t c1cea4d6-9aec-445c-8179-bc5543358876",
    "prebuild": "rimraf dist",
    "build-with-files":
      "npm run prebuild && babel --copy-files --out-dir dist --ignore *.test.js src",
    "build": "npm run prebuild && babel --out-dir dist --ignore *.test.js src",
    "test": "mocha --compilers js:babel-register src/index.test.js -w",
    "test:single": "nyc mocha --compilers js:babel-register src/index.test.js"
  },
  "babel": {
    "presets": ["es2015", "stage-2"]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/littletower/business-hours.js.git"
  },
  "author": "Stefano Tron",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/littletower/business-hours.js/issues"
  },
  "homepage": "https://github.com/littletower/business-hours.js#readme",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "chai": "4.2.0",
    "codecov": "3.2.0",
    "codecov.io": "0.1.6",
    "istanbul": "0.4.5",
    "mocha": "5.2.0",
    "nyc": "13.3.0",
    "rimraf": "2.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && npm run test:single"
    }
  },

  "dependencies": {
    "lodash": "4.17.11",
    "mockdate": "2.0.2",
    "moment-timezone": "0.5.23"
  }
}
